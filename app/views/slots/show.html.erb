<%= render "layouts/header"%>

<table>
  <tr>
  <th>BookingCode</th>
  <td>卸し時の実入り搬入コンテナ番号</td>
  <td>卸し時の空バン返却コンテナ番号</td>
  <td>積み時のBooking番号</td>
  <td>積み時の輸入実入りコンテナ番号</td>
  <td>off_action</td>
  <td>on_action</td>
  <td>お名前</td>
  <td>会社名</td>
  <td>電話番号</td>
  <td>更新</td>
  <td>削除</td>
  </tr>
<% @bookings.each do |booking| %>
    <tr>
      <th><%= booking.booking_code%></th>
      <td><%= booking.on_imp_laden_pick%></td>
      <td><%= booking.on_exp_booking_num%></td>
      <td><%= booking.off_exp_laden_in%></td>
      <td><%= booking.off_imp_empty_return%></td>
      <td><%= booking.off_action%></td>
      <td><%= booking.on_action%></td>
      <td><%= booking.user.name %></td>
      <td><%= booking.user.company %></td>
      <td><%= booking.user.phone %></td>
      <td><%= link_to '更新', edit_booking_path(booking.id), method: :get %></td>
      <td><%= link_to '削除', booking_path(booking.id), method: :delete %></td>
    </tr>
  <% end %>
</table>

<%= render "layouts/footer"%>